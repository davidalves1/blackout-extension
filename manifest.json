{
  "manifest_version": 3,
  "name": "Page Blackout",
  "description": "Enable dark theme to current page",
  "version": "1.0",
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "64": "assets/icon-64.png"
  },
  "action": {
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "64": "assets/icon-64.png"
    }
  },
  "background": {
    "service_worker": "main.js"
  },
  "permissions": ["activeTab", "scripting", "notifications"]
}
